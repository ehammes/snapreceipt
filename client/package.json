    "pg": "^8.16.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.10.1",
    "react-scripts": "5.0.1",
    "recharts": "^3.6.0",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
